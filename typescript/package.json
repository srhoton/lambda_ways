{
  "name": "typescript-lambda-crud",
  "version": "1.0.0",
  "description": "TypeScript AWS Lambda for handling CRUD requests from API Gateway",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "aws",
    "lambda",
    "typescript",
    "crud",
    "api-gateway"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "2.9.0",
    "@aws-lambda-powertools/metrics": "2.9.0",
    "@aws-lambda-powertools/tracer": "2.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/aws-lambda": "8.10.145",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.0",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "aws-lambda": "1.0.7",
    "eslint": "9.16.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-sonarjs": "2.0.4",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
}
